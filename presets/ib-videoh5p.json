{
    "key": "video-h5p",
    "name": "Contenidor video H5P",
    "category": "video",
    "instructions": "Defineix un contenidor per a incrustar un video H5P on s'hi aplica un amplada màxima responsiva.\n",
    "template": "<p><br></p>\n<!--ini youtube-->\n<div class=\"iedib-video-container\" {{#if}}[ampladaMax>100]style=\"max-width:{{ampladaMax}}px\"{{/if}}>\n  <div class=\"iedib-videoWrapper\">\n    <div class=\"h5p-placeholder\" contenteditable=\"false\"></div>\n  </div>\n    <div class=\"iedib-caption\">Vídeo: <em>{{titol}}</em></div>\n</div>\n<!--end youtube-->\n<p><br></p>\n",
    "parameters": [
        {
            "name": "ampladaMax",
            "value": 500,
            "title": "Amplada màxima en px",
            "min": 100
        },
        {
            "name": "titol",
            "value": "Títol",
            "title": "Títol descriptiu."
        }
    ],
    "nou": true,
    "author": "Josep Mulet Pol <pep.mulet@gmail.com>",
    "version": "3.2.0"
}